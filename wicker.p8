__lua__
function _init()

	state = "play"

	--player
	player = {}
	player.x = 60
	player.y = 106
	player.flip = false
	player.spr = 5
	player.state = "idle"
	invin = false
	invin_time = 60
	
	--animation
	stimer = 0
	
	--apples
	apples = {}
	bapples = {}
	
	--enemy
	ducks= {}
	dtimer = 0
	
	--game
	score = 0
	gtimer = 0
	seconds = 0
	lane1 = 106
	lane2 = 118
	
	sfx(3)
end


function _update()
	game_timer()
	if seconds > 2 and seconds < 63 then
 	update_game()
 else
 	end_game()
 end
end

function _draw()
	if seconds < 63 then
		draw_game()
	else
		draw_end()
	end
end
