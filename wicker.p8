-- draw

function draw_game()
 cls(12)
 map()
	spr(player.spr,player.x,player.y,1,1,player.flip)
	draw_spr_col(apple,apples)
	draw_spr_col(bapple,bapples)
	draw_spr_col(duck,ducks)
	print('score: '..score, 10, 2, 0)
	if seconds > 2 then
		print('time: '..seconds - 2  , 90, 2, 0)
	end
end 

function draw_end()
cls(1)
print('score: '..score, 45, 50, 7)
print("press ❎ to play again", 25, 70, 7)
end
