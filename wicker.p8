-- update

function update_game()
	update_timers()
	anim_duck()
	check_invin()
	
	if stimer == flr(rnd(4))+2 then
		spawn_apple()
		spawn_bapple()
	end
	
	if dtimer == 25 then
		spawn_duck()
	end
	
	if not btn(➡️) and not btn(⬅️) then
		if invin == true then
			player.spr = 8
		else
			player.spr = 5
		end
	end

 if btn(➡️) then
		player_mv("right")
	end
	
	if btn(⬅️) then
		player_mv("left")
	end
	
	if btn(⬆️) then
		player_mv("up")
	end
	
	if btn(⬇️) then
		player_mv("down")
	end
	
	for apple in all(apples) do
		
		apple.timer += 1
		
		if apple.timer < 30 then
			apple.y += 0
		else
			apple.y += apple.speed
		end
		
		if invin == false then
			if collision(apple,player) then
				score += 20
				sfx(0)
				del(apples,apple)
			end
		end
		
		if apple.y > 128 then
			del(apples,apple)
		end
	end
	
	for bapple in all(bapples) do
	
		bapple.timer += 1
		
		if bapple.timer < flr(rnd(30))+10 then
			bapple.y += 0
		else
			bapple.y += bapple.speed
		end
		
		if invin == false then
			if collision(bapple,player) then
				score -= 5
				sfx(1)
				del(bapples,bapple)
			end
		end
		
		if bapple.y > 128 then
			del(bapples,bapple)
		end
		
	end
	
	for duck in all(ducks) do
		duck.x += 1.5
		
		if collision(duck,player) then
			if invin == false then
				invin = true 
				score -= 10
				sfx(2)
				del(ducks,duck)
			end
		end
		
		if duck.x > 128 then
			del(ducks,duck)
		end	
	end

	if seconds == 120 then
		end_game()
	end

end
