-- player

function player_mv(dir)

	if stimer%5 == 0 then
		if invin == true then
			player.spr = 8
		else
			if player.spr < 7 then
	 		player.spr += 1
	 	else
	 		player.spr = 6
	 	end
 	end
 end

--right
	if dir == "right" then
	
		player.x += 3
		player.flip = false
		
	end
	
--left
	if dir == "left" then
	
		player.x -= 3
		player.flip = true
		
	end
	
--down
	if dir == "down" then
		player.y = lane2
	end
	
--up
	if dir == "up" then
		player.y = lane1
	end
	
--collision
	if player.x+8 > 128 then
		player.x = 120
	end
	
	if player.x < 0 then
		player.x = 0
	end

end


-->8
-- helper

function update_timers()

	if stimer < 19 then
		stimer += 1
	else
		stimer = 0
	end
	
	if dtimer < 30 then
		dtimer += 1
	else
		dtimer = 0
	end	

end

function game_timer()
	gtimer += 1
 
 if gtimer % 30 == 0 then
 	seconds += 1
 end
 
end

function collision(a,b)

	local a_left = a.x
	local a_top = a.y
	local a_right = a.x+7
	local a_bottom = a.y+7
	
	local b_left = b.x
	local b_top = b.y
	local b_right = b.x+7
	local b_bottom = b.y+7
	
	if a_top > b_bottom then return false end
	if b_top > a_bottom then return false end
	if a_left > b_right then return false end
	if b_left > a_right then return false end
	
	return true
	
end

function spawn_apple()
	local apple = {}
	apple.x = flr(rnd(110))+10
	apple.y = flr(rnd(40))+15
	apple.spr = 3
	apple.timer = 0
	apple.speed = rnd(3.5)+1
	add(apples,apple)
end

function spawn_bapple()
	local bapple = {}
	bapple.x = flr(rnd(110))+10
	bapple.y = flr(rnd(40))+15
	bapple.spr = 0
	bapple.timer = 0
	bapple.speed = rnd(3.5)+1
	add(bapples,bapple)
end

function spawn_duck()
	rand = flr(rnd(2))+1
	local duck = {}
	duck.x = -5
	if rand == 2 then
		duck.y = lane1
	else
		duck.y = lane2
	end
	duck.spr = 24
	add(ducks,duck)
end

function draw_spr_col(ob,col)
	for ob in all(col) do
		spr(ob.spr,ob.x,ob.y)
	end
end

function check_invin()
	if invin == true then
		if invin_time >= 0 then
			invin_time -= 1
		else
			invin = false
			invin_time = 60
		end
	end
end

function end_game()
	if seconds > 12 then
		if btn(❎) then
			_init()
		end
	end
end
